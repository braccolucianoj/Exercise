{
  "name": "exercise",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/braccolucianoj/Exercise",
  "author": "Luciano J. Bracco",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "nodemon",
    "test": "jest --verbose",
    "test:watch": "jest --verbose --watch"
  },
  "dependencies": {
    "config": "3.3.2",
    "lodash": "4.17.20",
    "uuid": "8.3.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "7.12.7",
    "@types/config": "0.0.37",
    "@types/jest": "26.0.15",
    "@types/lodash": "4.14.165",
    "@types/node": "14.14.9",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "4.8.1",
    "eslint": "7.14.0",
    "eslint-config-airbnb-typescript": "12.0.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.1.4",
    "jest": "26.6.3",
    "nodemon": "2.0.6",
    "prettier": "^2.2.0",
    "ts-jest": "26.4.4",
    "ts-node": "9.0.0",
    "typescript": "4.1.2"
  },
  "watch": [
    "src"
  ],
  "ext": ".ts,.js",
  "ignore": [],
  "exec": "ts-node ./src/index.ts",
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.(ts)$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "\\.snap$"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/node_modules",
      "."
    ],
    "testEnvironment": "jsdom",
    "globals": {
      "ts-jest": {
        "tsconfig": "./tsconfig.json",
        "babelConfig": true
      }
    }
  }
}
